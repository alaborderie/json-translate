[  {    "type": "post",    "url": "/translate/api/v1",    "title": "Translate your JSON to any language",    "name": "Translate",    "group": "Translate",    "version": "1.0.0",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "Object",            "optional": false,            "field": "json",            "description": "<p>Contains the JSON you want to translate</p>"          },          {            "group": "Parameter",            "type": "string",            "optional": false,            "field": "sourceLang",            "description": "<p>The source language you are using</p>"          },          {            "group": "Parameter",            "type": "string[]",            "optional": false,            "field": "exportLangs",            "description": "<p>An array containing each language you want to translate to as a string (i.e &quot;en&quot; for english)</p>"          },          {            "group": "Parameter",            "type": "string",            "optional": false,            "field": "apiKey",            "description": "<p>Your google translate API Key</p>"          }        ]      },      "examples": [        {          "title": "Request-Example:",          "content": "{\n    \"json\": {\n        \"test\": \"Ce texte était en français\"\n    },\n    \"sourceLang\": \"fr\",\n    \"exportLangs\": [\n        \"en\",\n        \"es\"\n    ]\n}",          "type": "json"        }      ]    },    "success": {      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n    \"en\": {\n        \"test\": \"This text was in French\"\n    },\n    \"es\": {\n        \"test\": \"Este texto estaba en francés\"\n    }\n}",          "type": "json"        }      ]    },    "error": {      "examples": [        {          "title": "Error-Response:",          "content": "HTTP/1.1 409 KO\n{\n    \"error\": \"Invalid API Key\"\n}",          "type": "json"        }      ]    },    "filename": "routes/translate.js",    "groupTitle": "Translate"  }]
